// Phim F1 --> Develope: Inspect Editor Tokens And Scopes --> Paste
"editor.tokenColorCustomizations": {
        "[Default High Contrast]": {
            "textMateRules": [
                {
                    "scope": ["entity.name.tag", "entity.name.tag.css", "support.constant.property-value", "variable", "punctuation.section.function.begin.bracket.round.css", "punctuation.section.function.end.bracket.round.css", "punctuation.section.property-list.begin.bracket.curly.css", "punctuation.section.property-list.end.bracket.curly.css", "storage.type.function.arrow.js", "meta.brace.round.js"],
                    "settings": {
                        "foreground": "#fff",
                    }
                },
                {
                    "scope": ["entity.other.attribute-name", "entity.other.attribute-name.class.css", "constant.numeric", "keyword.other.unit", "entity.other.attribute-name.pseudo-class.css"],
                    "settings": {
                        "foreground": "#df3079",
                    }
                },
                {
                    "scope": ["string", "string.regexp", "support.type.property-name"],
                    "settings": {
                        "foreground": "#00a67d",
                    }
                },
                {
                    "scope": ["meta.tag.metadata.doctype.html", "storage.type", "keyword.control"],
                    "settings": {
                        "foreground": "#2e95d3",
                    }
                },
                {
                    "scope": ["support.function", "support.constant.media", "entity.other.attribute-name.pseudo-element.css"],
                    "settings": {
                        "foreground": "#e9950c",
                    }
                },
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#f22c3d",
                    }
                },
                {
                    "scope": "comment",
                    "settings": {
                        "foreground": "#ffffff80",
                    }
                }
            ]
        }
    },